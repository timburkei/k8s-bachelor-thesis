                <mxCell id="294" value="&lt;h1&gt;&lt;font color=&quot;#ffffff&quot;&gt;Controller Manager&lt;/font&gt;&lt;/h1&gt;&lt;div&gt;&lt;font color=&quot;#ffffff&quot;&gt;Führt Controller aus, um das Verhalten der Kubernetes-API zu implementieren.&lt;/font&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;font color=&quot;#ffffff&quot;&gt;&lt;div&gt;&lt;span style=&quot;background-color: initial;&quot;&gt;&lt;br&gt;&lt;/span&gt;&lt;/div&gt;&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font color=&quot;#ffffff&quot;&gt;&lt;br&gt;&lt;/font&gt;&lt;/div&gt;" style="text;html=1;strokeColor=#2875e2;fillColor=none;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;shadow=0;labelBackgroundColor=none;sketch=0;strokeWidth=1;fontSize=16;fontColor=#000000;glass=0;dashed=1;dashPattern=12 12;" vertex="1" parent="302">
